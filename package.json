{
  "name": "owncloud_phoenix",
  "title": "ownCloud Phoenix",
  "icon": "folder",
  "version": "0.1.0",
  "description": "A safe home for all your data",
  "main": "core/core.bundle.js",
  "scripts": {
    "build:dev": "webpack -d --config webpack.dev.js",
    "build:prod": "webpack -p --config webpack.prod.js",
    "install": "node build.js && yarn run build:dev",
    "watch": "yarn run build:dev && webpack-dev-server --progress --colors --watch --config webpack.dev.js --public $SERVER_HOST",
    "lint": "eslint src/**/*.vue src/**/*.js --color --global requirejs --global require",
    "lint-fix": "eslint src/**/*.vue src/**/*.js --color --global requirejs --global require --fix",
    "accessibility-tests": "node node_modules/axe-cli/axe-cli --exit --disable page-has-heading-one,meta-viewport",
    "accessibility-tests-drone": "node node_modules/axe-cli/axe-cli --exit --disable page-has-heading-one,meta-viewport",
    "acceptance-tests": "cucumber-js --require-module babel-core/register --require tests/acceptance/setup-local.js --require tests/acceptance/stepDefinitions --format node_modules/cucumber-pretty tests/acceptance/features",
    "acceptance-tests-drone": "cucumber-js --require-module babel-core/register --require tests/acceptance/setup-drone.js --require tests/acceptance/stepDefinitions --format node_modules/cucumber-pretty tests/acceptance/features"
  },
  "author": "Felix Heidecke",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/owncloud/phoenix",
  "devDependencies": {
    "axe-cli": "^3.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^2.44.0",
    "css-loader": "^1.0.1",
    "cucumber": "^5.0.2",
    "cucumber-pretty": "^1.4.0",
    "easygettext": "^2.7.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.5",
    "file-loader": "^2.0.0",
    "git-hooks": "^1.1.10",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "nightwatch": "^1.0.11",
    "nightwatch-api": "^1.0.0",
    "node-sass": "^4.10.0",
    "parse-json": "^4.0.0",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "vue-loader": "15.4.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.1",
    "webpack-copy-plugin": "^0.0.4",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-owncloud-client": "github:owncloud/js-owncloud-client#master",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^4.0.2",
    "requirejs": "^2.3.6",
    "uikit": "^3.0.0-rc.9",
    "vue": "^2.5.16",
    "vue-authenticate": "^1.3.4",
    "vue-axios": "^2.1.4",
    "vue-drag-drop": "^1.1.4",
    "vue-events": "^3.1.0",
    "vue-gettext": "^2.1.1",
    "vue-router": "^3.0.2",
    "vuetify": "^1.3.11",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0",
    "vuex-router-sync": "^5.0.0"
  }
}
